-- Used to calculate the tool setter position


function m901()
	local inst = mc.mcGetInstance("m901 calculate tool setter position") --Get the instance of Mach
	local is_ok, message = pcall(ns.AutoSetToolHeight, false, true)
	if is_ok then
		mc.mcCntlSetLastError(inst, message)
	else
		mc.mcCntlMacroAlarm(inst, 6, message)
	end
end


--This function is used to allow the debugging to be done in the mcLua editor
if (mc.mcInEditor() == 1) then
    m901()
end  
